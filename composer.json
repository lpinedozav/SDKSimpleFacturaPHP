{
    "name": "simplefactura/sdk",
    "description": "SDK para integrar Simple Factura",
    "version": "1.0.0",
    "readme": "README.md",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/lpinedozav/SDKSimpleFacturaPHP.git",
    "autoload": {
        "psr-4": {
            "SDKSimpleFactura\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Luis Pinedo",
            "email": "pinedo.zavala.bas@gmail.com"
        },
        {
            "name": "Carlos Perea",
            "email": "pereacarlos685@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^7.9",
